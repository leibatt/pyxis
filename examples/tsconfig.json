{
  "compilerOptions": {
    "outDir": "../build-test",
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ],
    "lib": [
      "ES2021.String"
    ]
  },
  "files": [
    "relationships/KDEExample.ts",
    "relationships/KNNExample.ts",
    "relationships/decisionTreeExample.ts",
    "relationships/decisionTreeExample2.ts",
    "relationships/gaussianNaiveBayesExample.ts",
    "relationships/isolationForestExample.ts",
    "relationships/linearRegressionExample.ts",
    "transformations/arquero.ts",
    "transformations/vega.ts",
    "use_cases/amar.ts",
    "use_cases/north.ts",
    "use_cases/mathisen.ts",
    "use_cases/battle/battle.ts",
    "use_cases/zgraggen/zgraggen.ts"
  ],
  "include": ["../src/**/*.ts", "../types/*.d.ts"]
}
