    "json-schema": "^0.4.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.6",
    "node-fetch": "^3.2.9",
    "node-notifier": "^10.0.1",
    "path-parse": "^1.0.7",
    "rollup": "^2.77.0",
    "rollup-plugin-polyfill-node": "^0.10.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "tmpl": "^1.0.5",
