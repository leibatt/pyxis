{
  "name": "@leibatt/pyxis",
  "version": "0.2.5",
  "description": "A language for specifying visual analysis insights, objectives and tasks.",
  "main": "build/index.js",
  "browser": "umd/index.umd.js",
  "module": "build/src/index",
  "types": "build/src/index.d.ts",
