{
  "name": "@leibatt/pyxis",
  "version": "0.0.1",
  "description": "A language for specifying visual analysis insights, objectives and tasks.",
  "main": "build/index.js",
  "module": "build/src/index",
  "types": "build/src/index.d.ts",
  "repository": "git@github.com:leibatt/pyxis.git",
  "author": "Leilani Battle, Alvitta Ottley",
  "license": "MIT",
  "files": [
    "README.md",
    "build",
    "src",
    "lib",
    "types",
    "tsconfig.json"
  ],
  "scripts": {
    "clean": "rm -rf build/* build-test/*",
    "build": "yarn build-src && yarn build-examples && yarn build-datasets",
    "build-src": "tsc",
    "build-examples": "tsc -p examples/tsconfig.json",
    "build-datasets": "cp -R datasets build-test",
    "test": "jest --collectCoverage=true && yarn lint",
    "lint": "eslint .",
    "bundle": "rollup --config"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-typescript": "^8.3.3",
    "arquero": "^4.8.8",
    "eslint-plugin-jest": "^24.1.3",
    "isolation-forest": "^0.0.9",
    "jest": "^26.6.3",
    "json-schema": "^0.4.0",
    "minimist": "^1.2.6",
    "ml-cart": "^2.1.1",
    "ml-knn": "^3.0.0",
    "ml-naivebayes": "^4.0.0",
    "rollup": "^2.76.0",
    "typescript": "^4.1.2",
    "uuid": "^8.3.2",
    "vega-statistics": "^1.7.10"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "ml-regression-multivariate-linear": "^2.0.3",
    "ts-jest": "^26.4.4",
    "vega": "^5.17.3"
  }
